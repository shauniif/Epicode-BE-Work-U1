@model Dictionary<string, int>

@{
    ViewData["Title"] = "Spedizioni per Città";
}

<h2>Spedizioni per Città</h2>

@if (Model.Any())
{
    <table class="table">
        <thead>
            <tr>
                <th>Città</th>
                <th>Numero di Spedizioni</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Key</td>
                    <td>@item.Value</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>Nessuna spedizione trovata.</p>
}